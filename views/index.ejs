        <!-- contacts -->
        <div class="h-screen w-full bg-transparent overflow-clip" id="contacts">
            <!-- feedback form -->
            <!-- <img class=" -z-10" src="./images/aboutSide.svg" alt=""> -->
            <div class="h-[50%] w-full px-[10%] grid grid-cols-2">
                <div class="col-span-1 flex justify-center items-center">
                    <img class="w-[60%]" src="../images/callCenter.png" alt="">
                </div>
                <div class="col-span-1">
                    <div class="p-[10%]">
                        <form action="#" class="flex flex-col gap-1">
                            <label for="email">Email</label><br>
                            <input class="border-2 border-solid border-emerald-500 bg-emerald-50 px-3 rounded-lg h-10"
                                type="email" placeholder="Enter your Email"><br>
                            <label for="Query">Query</label>
                            <textarea class="border-2 border-solid border-emerald-500 bg-emerald-50 px-3 rounded-lg"
                                name="" id="" cols="30" rows="5" placeholder="Enter your query"></textarea>
                            <button
                                class="text-white w-[30%] border-2 border-[#92af19] text-lg transition ease-in-out hover:scale-110 hover:text-[#92af19] px-5 hover:bg-white py-1 bg-[#92AF19] rounded-3xl hover:translate-y-1"
                                type="submit">Send</button>
                        </form>
                    </div>
                </div>
            </div>

            <!-- contacts link section -->
            <div
                class="h-[50%] w-full bg-emerald-900  py-[100px] flex flex-col justify-center items-center gap-5 text-white">
